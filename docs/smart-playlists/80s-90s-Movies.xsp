<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<smartplaylist type="movies">
    <n>80s and 90s Movies</n>
    <match>all</match>
    <rule field="year" operator="greaterthan">
        <value>1979</value>
    </rule>
    <rule field="year" operator="lessthan">
        <value>2000</value>
    </rule>
</smartplaylist>
