<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<smartplaylist type="movies">
    <n>Movies - Date Night</n>
    <match>all</match>
    <rule field="genre" operator="is">
        <value>Romance</value>
    </rule>
    <rule field="rating" operator="greaterthan">
        <value>6</value>
    </rule>
</smartplaylist>
