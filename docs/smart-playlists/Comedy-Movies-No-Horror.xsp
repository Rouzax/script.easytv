<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<smartplaylist type="movies">
    <n>Comedy Movies No Horror</n>
    <match>all</match>
    <rule field="genre" operator="is">
        <value>Comedy</value>
    </rule>
    <rule field="genre" operator="isnot">
        <value>Horror</value>
    </rule>
</smartplaylist>
