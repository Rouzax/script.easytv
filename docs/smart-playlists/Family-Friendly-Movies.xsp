<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<smartplaylist type="movies">
    <n>Family Friendly Movies</n>
    <match>one</match>
    <rule field="mpaa" operator="contains">
        <value>G</value>
    </rule>
    <rule field="mpaa" operator="contains">
        <value>PG</value>
    </rule>
    <rule field="mpaa" operator="doesnotcontain">
        <value>PG-13</value>
    </rule>
</smartplaylist>
